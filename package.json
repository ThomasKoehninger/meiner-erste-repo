      "react-helmet": "^5.2.1",
      "react-icons": "^3.7.0",
      "typescript": "^3.5.1"
    },
    "devDependencies": {
      "@types/node": "^12.0.7",
      "@types/react": "^16.8.19",
      "@types/react-dom": "^16.8.4",
      "@types/react-helmet": "^5.0.8",
      "gh-pages": "^2.0.1",
      "tslint": "^5.17.0"
